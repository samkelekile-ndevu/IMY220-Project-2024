#Install node lts and npm in linux cli:
    curl -fsSL https://deb.nodesource.com/setup_lts.x | sudo -E bash -
    sudo apt-get install nodejs -y

#Install project packaged:
    npm install

#Build Project:
    npm run build

#Run Project:
    npm run start


##########################
#Docker Commands
#########################


#Build Image
docker image build -t imy220-projectd1-image:latest

#Login
docker login

#Push image
docker push u21593681/imy220-projectd1-image

#Run
docker run -dp 8000:3000 --name imy220-projectd1-container imy220-projectd1:latest

#Show images
docker ps

#Run on browser
http://localhost:8000
